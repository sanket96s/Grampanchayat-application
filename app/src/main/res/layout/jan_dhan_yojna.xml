        <TextView
            android:id="@+id/textView9"
            android:layout_width="383dp"
            android:layout_height="170dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="133dp"
            android:text="प्रधानमंत्री जनधन योजना (PMJDY) प्रत्येक परिवाराचा या योजनेत समावेश करण्यावर भर देते. यापूर्वीची योजना खेड्यांचा विचार करीत होती. ही योजना ग्रामीण आणि शहरी भागाला समाविष्ट करीत आहे. यापूर्वीची योजना ज्या खेड्यांची लोकसंख्या २०००पेक्षा जास्त आहे त्याच खेड्यांना या स्वाभिमान योजनेंचा लाभ होत होता. पण पी.एम.जे.डी.वाय. या योजनेंतर्गत १००० ते १५०० परिवार असलेल्या प्रत्येक उपसेवा क्षेत्रात बँकिंग सेवा उपलब्ध होऊन सारा देश या योजनेंतर्गत गुंतला जाणार आहे. प्रत्येक पाच किलोमीटरच्या अंतरावर या सुविधा या योजनेंतर्गत संपूर्ण देशभर उपलब्ध करून देण्यात येणार आहेत."
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteY="159dp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="247dp"
            android:layout_height="103dp"
            android:layout_marginLeft="135dp"
            android:layout_marginTop="15dp"
            android:text="प्रधानमंत्री जनधन योजना (PMJDY) "
            android:textAppearance="@style/TextAppearance.AppCompat.Display1"
            android:textStyle="bold"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="144dp"
            tools:layout_editor_absoluteY="25dp" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="95dp"
            android:layout_height="106dp"
            android:layout_marginTop="5dp"
            android:layout_marginStart="16dp"
            app:layout_constraintStart_toStartOf="parent"
