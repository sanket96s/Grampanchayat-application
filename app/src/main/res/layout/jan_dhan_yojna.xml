        <TextView
            android:id="@+id/textView18"
            android:layout_width="wrap_content"
            android:layout_height="173dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="605dp"
            android:text="1.	शून्य बॅलन्स खाते&#xA;2.	रुपे डेबिट कार्ड&#xA;3.	रु. १ लाख अपघात विमा&#xA;4.	रु. ३०,००० जीवन विमा&#xA;5.	रु. १०,००० ओव्हरड्राफ्ट सुविधा&#xA;6.	वित्तीय समावेशन&#xA;7.	सरकारी योजनांचा थेट लाभ&#xA;8.	मोफत मोबाईल बँकिंग सुविधा&#xA;9.	आंतरराष्ट्रीय व्यवहार (रुपे कार्डद्वारे)&#xA;"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="59dp"
            tools:layout_editor_absoluteY="659dp" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginTop="585dp"
            android:layout_marginLeft="15dp"
            android:text="फायदे :"
            android:textColor="#2C2727"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteY="663dp" />

        <View
            android:id="@+id/bold_line4"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:background="#000000"
            android:visibility="visible"
            android:layout_marginTop="433dp"
            app:layout_constraintBottom_toBottomOf="parent"
